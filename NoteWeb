<p><em>This is the code in (Jb) from this face:</em></p>
<p>
	var Face3 = function(x, y, FS, n) { <br>
    strokeWeight(0.001); <br>
    ellipse(x, y, FS, FS); <br>
    strokeWeight(n);<br>
    fill(5, 4, 5); <br>
    line(x-0.25*FS, y-0.15*FS, x-0.1*FS, y-0.15*FS); <br>
    line(x-0.1*FS, y-0.15*FS, x+-0.25*FS, y-0.25*FS); <br>
    ellipse(x+0.2*FS, y-0.15*FS, 0.1*FS, -0.2*FS); <br>
    fill(255, 255, 255); <br>
    arc( x+0.01*FS, y+0.05*FS, 0.3*FS, 0.2*FS, 61, 180);

};
</p>
