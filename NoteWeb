		var Face2 = function(x, y, FS, n) { <br>
    strokeWeight(0.001); <br>
    ellipse(x, y, FS, FS); <br>
    strokeWeight(n); <br>
    line(x-0.25*FS, y-0.15*FS, x-0.1*FS, y-0.15*FS); <br>
    line(x+0.1*FS, y-0.15*FS, x+0.25*FS, y-0.15*FS); <br>
    line(x-0.25*FS, y+0.18*FS, x+ 0.25*FS, y+0.18*FS); <br>
    fill(10, 9, 10); <br>
    ellipse(x-0.1*FS, y-0.1*FS, 0.1*FS, 0.1*FS); <br>
    ellipse(x+0.25*FS, y-0.1*FS, 0.1*FS, 0.1*FS);

};
</p>

